import { CommonEntity } from '@/common/entities/common.entity';
import { Column, Entity } from 'typeorm';

/**
 * component
 */
@Entity('{{kebabCase name}}')
export class {{pascalCase name}} extends CommonEntity {
  @Column({
    comment: 'nickname',
    default: '',
  })
  name: string;

  @Column({
    comment: 'tel',
    nullable: true,
  })
  tel: string;

  @Column({
    comment: 'avatar',
    nullable: true,
  })
  avatar: string;

  @Column({
    comment: 'password',
  })
  password: string;

  @Column({
    comment: 'account',
  })
  account: string;

  @ManyToOne(() => Organization, (org) => org.{{kebabCase name}}s, {
    cascade: true,
  })
  org: Organization;
}
